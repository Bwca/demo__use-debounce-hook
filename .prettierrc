{
  "arrowParens": "always",
  "parser": "flow",
  "printWidth": 90,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 4,
  "trailingComma": "es5",
  "useTabs": false,
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "options": {
        "parser": "typescript",
        "endOfLine": "auto"
      }
    },
    {
      "files": [
        "*.scss",
        "*.module.scss"
      ],
      "options": {
        "parser": "scss"
      }
    }
  ]
}